set -l commands help receive send ssh

complete -c wormhole --no-files
complete -c wormhole -l help

complete -n "not __fish_seen_subcommand_from $commands" -c wormhole -a receive -d "Receive a text message, file, or directory (from 'wormhole send')"

complete -n "not __fish_seen_subcommand_from $commands" -c wormhole -a send -d "Send a text message, file, or directory"

complete --force-files -n "__fish_seen_subcommand_from send" -c wormhole -a "(__fish_complete_path)"
